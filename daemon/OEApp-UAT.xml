<!-- 
  Operational Excellence App - UAT Windows Service
  URL: https://oeapp-uat.gmrlapps.com
  
  Setup Instructions:
  1. Copy WinSW.exe to this folder as OEApp-UAT.exe
  2. Run as Administrator: OEApp-UAT.exe install
  3. Run: OEApp-UAT.exe start
  
  Commands:
    OEApp-UAT.exe install   - Install the service
    OEApp-UAT.exe uninstall - Remove the service
    OEApp-UAT.exe start     - Start the service
    OEApp-UAT.exe stop      - Stop the service
    OEApp-UAT.exe restart   - Restart the service
    OEApp-UAT.exe status    - Check service status
-->
<service>
    <id>OEApp-UAT</id>
    <name>Operational Excellence App (UAT)</name>
    <description>Operational Excellence Application - UAT Environment (https://oeapp-uat.gmrlapps.com)</description>
    <executable>C:\Program Files\nodejs\node.exe</executable>
    <argument>--harmony</argument>
    <argument>F:\Operational Excellence Appliaction\node_modules\node-windows\lib\wrapper.js</argument>
    <argument>--file</argument>
    <argument>F:\Operational Excellence Appliaction\app.js</argument>
    <argument>--scriptoptions=</argument>
    <argument>--log</argument>
    <argument>OEApp-UAT wrapper</argument>
    <argument>--grow</argument>
    <argument>0.25</argument>
    <argument>--wait</argument>
    <argument>1</argument>
    <argument>--maxrestarts</argument>
    <argument>3</argument>
    <argument>--abortonerror</argument>
    <argument>n</argument>
    <argument>--stopparentfirst</argument>
    <argument>undefined</argument>
    <logpath>F:\Operational Excellence Appliaction\logs</logpath>
    <logmode>rotate</logmode>
    <stoptimeout>5sec</stoptimeout>
    <env name="NODE_ENV" value="uat"/>
    <env name="PORT" value="3010"/>
    <serviceaccount>
        <domain>PAPPS</domain>
        <user>LocalSystem</user>
        <password></password>
    </serviceaccount>
    <workingdirectory>F:\Operational Excellence Appliaction</workingdirectory>
</service>
